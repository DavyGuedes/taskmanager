{
  "expo": {
    "name": "Task Manager",
    "slug": "taskmanager",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "io.github.davyguedes.taskmanager"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.github.davyguedes.taskmanager"
    }
  }
}
